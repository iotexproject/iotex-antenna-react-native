{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "_note": "npm run lint && npm run tsc-test && ",
    "test": "npm run lint && jest",
    "typecheck": "tsc --noEmit",
    "lint": "pretty-quick --staged && tslint --fix --project tsconfig.json -t stylish",
    "qualitycheck": "npm-run-all typecheck tslint",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"src/{*.ts,**/*.ts,*.tsx,**/*.tsx}\"\n"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react"
        }
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@ant-design/react-native": "^3.1.10",
    "@expo/samples": "2.1.1",
    "@expo/vector-icons": "^9.0.0",
    "@tradle/react-native-http": "^2.0.1",
    "@types/expo": "31.0.8",
    "@types/react-navigation": "2.13.10",
    "@types/react-redux": "7.0.0",
    "axios": "^0.19.0",
    "browserify-zlib": "^0.1.4",
    "buffer": "^4.9.1",
    "dns.js": "^1.0.1",
